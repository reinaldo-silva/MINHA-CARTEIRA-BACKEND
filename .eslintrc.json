{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": ["error", {"endOfLine":"auto"}],
      "class-methods-use-this": "off",
      "camelcase": "off"
    }
}
